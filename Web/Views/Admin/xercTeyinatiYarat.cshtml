@model XercTeyinati

<div class="row">
    <div class="col-md-6">
        <h5>Xerc Teyinati Yarat</h5><hr>
        <form asp-controller="admin" asp-action="xercTeyinatiYarat" method="post">
            <div class="form-group">
                <label>Xerc Teyinati</label>
                <input type="text" asp-for="@Model.XTeyinati"  class="form-control" />
